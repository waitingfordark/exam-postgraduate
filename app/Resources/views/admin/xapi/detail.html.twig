{% extends 'old-bootstrap-modal-layout.html.twig' %}

{% block title %}
  {{ '推送详情'|trans }}
{% endblock %}

{% block body %}
  <table class="table cd-table table-striped order-detail-table">
    <tr>
      <td width="25%">{{ '推送id'|trans }}</td>
      <td width="75%">{{ statement.uuid }}</td>
    </tr>
    <tr>
      <td width="25%">{{ '推送状态'|trans }}</td>
      <td width="75%">
        {{ statement.status }}
      </td>
    </tr>
    <tr>
      <td width="25%">{{ 'xAPI版本'|trans }}</td>
      <td width="75%">
        {{ statement.version }}
      </td>
    </tr>
    <tr>
      <td width="25%">{{ '推送主体'|trans }}</td>
      <td width="75%">
        {{ statement.data|json_encode }}
      </td>
    </tr>
  </table>
{% endblock %}

{% block footer %}
  <button type="button" class="btn btn-primary" data-dismiss="modal">{{ 'form.btn.close'|trans }}</button>
{% endblock %}

