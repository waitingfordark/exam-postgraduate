{% extends 'admin/layout.html.twig' %}

{% set menu = 'admin_job_logs' %}

{% block main %}

    {% if logs %}
        <table class="table table-striped table-hover">
            <thead>
            <tr>
                <th>{{'时间'|trans}}</th>
                <th>{{'名称'|trans}}</th>
                <th>{{'来源'|trans}}</th>
                <th>{{'状态'|trans}}</th>
            </tr>
            </thead>
            <tbody>
            {% for log in logs %}
                {% include 'admin/jobs/log-tr.html.twig' with {log:log} %}
            {% endfor %}
            </tbody>

        </table>
    {% else %}
        <div class="empty">{{'暂无任务信息!'|trans}}</div>
    {% endif %}
    {{ admin_macro.paginator(paginator) }}

{% endblock %}